<!--index.wxml-->
<view class="container">
  <image src="../../images/logo.png"></image>
  <view class="section">
    <view class="username">
      <input bindinput="bindUsernameInput" placeholder="用户名"/>
    </view>

    <view class="password">
      <input password="true" bindinput="bindPasswordInput" placeholder="密码"/>
    </view>
    <button type="primary" bindtap="enter">登陆</button>
  </view>
</view>
